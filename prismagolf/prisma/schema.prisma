model bill {
  id        Int     @id(map: "PK_bill") @default(autoincrement())
  pelak     String? @db.Char(6)
  monthbill String? @db.Char(7)
  bedehi    Float?  @db.Money
  fine      Float?  @db.Money
  price     Float?  @db.Money
  pricebill Float?  @db.Money
}

